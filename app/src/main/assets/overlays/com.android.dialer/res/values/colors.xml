<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Background color of dialpad -->
    <color name="background_dialpad">@*common:color/grey_lighter</color>

    <!-- Pressed color of dialpad buttons -->
    <color name="background_dialpad_pressed">@*common:color/blue_light</color>

    <!-- Color of dialpad digits -->
    <color name="dialpad_digits_text_color">@*common:color/blue</color>

    <!-- Background color of action bars.  Ensure this stays in sync with packages/Telephony
         actionbar_background_color. -->
    <color name="actionbar_background_color">@*common:color/blue</color>

    <!-- Underline color of action bars.  Ensure this stays in sync with packages/Telephony
         actionbar_underline. -->
    <color name="actionbar_underline">@*common:color/blue</color>

    <!-- Background color of new dialer activity -->
    <color name="background_dialer_light">@*android:color/transparent</color>

    <!-- Background color for the "All Contacts" button in the favorites menu. -->
    <color name="all_contacts_button_color">@*common:color/blue</color>

    <!-- Background color for the "All Contacts" button in the favorites menu when pressed. -->
    <color name="all_contacts_button_pressed_color">@*common:color/blue_dark</color>
</resources>